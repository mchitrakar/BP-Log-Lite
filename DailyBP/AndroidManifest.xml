<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.life.dailybp"
    android:versionCode="2"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    
    <supports-screens android:smallScreens="true"
                  android:normalScreens="true"
                  android:largeScreens="true"
                  android:xlargeScreens="false"
                  />
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:debuggable="false"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.dailybp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.dailybp.RotatedViewActivity"
            android:label="@string/title_activity_rotated_view" >
        </activity>
        <activity
            android:name="com.example.dailybp.InputActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_input" >
            
        </activity>
        <activity
            android:name="com.example.dailybp.EditDataActivity"
            android:label="@string/title_activity_edit_data" >
        </activity>
        <activity
            android:name="com.example.dailybp.AboutActivity"
            android:label="@string/title_activity_about" >
        </activity>
        <activity
            android:name="com.example.dailybp.ListActivity"
            android:label="@string/title_activity_list" >
        </activity>
        <activity
            android:name="com.example.dailybp.ViewDataActivity"
            android:label="@string/title_activity_view_data" >
        </activity>
    </application>

</manifest>
